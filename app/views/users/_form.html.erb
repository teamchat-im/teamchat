<%= custom_form_with model: @user, html: { id: 'sign-up-form' } do |form| %>
  <%= form.text_field :username %>
  <%= form.text_field :email %>
  <%= form.password_field :password %>
  <%= form.password_field :password_confirmation %>
  <%= form.submit 'Sign up', class: 'button button--contained button--primary button--full-width' %>
<% end %>
